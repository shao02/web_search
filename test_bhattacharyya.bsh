#!/bin/bash

rm -f prf*.tsv
i=0
while read q ; do
	i=$((i + 1))
	prfout=prf-$i.tsv
	curl -s "localhost:25812/prf?query=$q&ranker=comprehensive&numdocs=10&numterms=50" > $prfout;
	echo "Finished $i"
	echo $q:$prfout >> prf.tsv
done < queries.tsv
java -cp target/classes edu.nyu.cs.cs2580.Bhattacharyya prf.tsv qsim.tsv
